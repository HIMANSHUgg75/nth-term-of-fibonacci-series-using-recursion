// Online C compiler to run C program online
#include <stdio.h>
int main() {
    int arr[7] = {1,2,3,4,5,6,7};
    
    
    int si, ei;
    printf("  ");
    scanf("%d %d", &si , &ei);
    
     int i = si;
     int j = ei;
         
    while(i<j){
        int temp = arr[i];
        arr[i] = arr[j];
        arr[j] = temp;
        
        i++;
        j--;
    } 
    
    for(int i=0; i<7; i++){
    printf("%d", arr[i]);
}

    return 0;
}

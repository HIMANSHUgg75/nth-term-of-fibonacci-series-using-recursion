 
#include <stdio.h>

int power(int a, int b){
   
   int x= 1;
   
   for(int i=1; i<=b; i++){
       x = x*a;
   }
       return x;
   
}


int main()
{
    int a,b;
    printf("  ");
    scanf("%d %d", &a, &b);
    
    
    
    int result  = power(a , b );
    printf("%d", result);
    return 0;
}
